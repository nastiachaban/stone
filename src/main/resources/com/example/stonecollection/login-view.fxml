<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane minHeight="-Infinity" minWidth="-Infinity" prefHeight="250.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/11.0.14-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.stonecollection.LoginController">
   <children>
      <GridPane layoutY="25.0" prefHeight="200.0" prefWidth="400.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>

          <Label alignment="CENTER" contentDisplay="RIGHT" prefHeight="28.0" prefWidth="194.0" text="Username" GridPane.columnIndex="0" GridPane.rowIndex="0">
            <font>
               <Font size="14.0" />
            </font></Label>
          <Label alignment="CENTER" prefHeight="20.0" prefWidth="201.0" text="Password" GridPane.columnIndex="0" GridPane.rowIndex="1">
            <font>
               <Font size="14.0" />
            </font></Label>

          <TextField prefHeight="36.0" fx:id="usernameTxt" prefWidth="177.0" GridPane.columnIndex="1" GridPane.rowIndex="0">
            <GridPane.margin>
               <Insets right="10.0" />
            </GridPane.margin></TextField>
          <PasswordField prefHeight="35.0" fx:id="passwordTxt" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="1">
            <GridPane.margin>
               <Insets right="10.0" />
            </GridPane.margin></PasswordField>

          <Button alignment="CENTER" onAction="#loginClick" mnemonicParsing="false" prefHeight="32.0" prefWidth="84.0" text="Log in" GridPane.columnIndex="0" GridPane.columnSpan="2" GridPane.rowIndex="2">
            <GridPane.margin>
               <Insets left="165.0" />
            </GridPane.margin>
            <font>
               <Font name="System Bold" size="14.0" />
            </font></Button>
      </GridPane>



   </children>
</AnchorPane>
